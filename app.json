{
  "expo": {
    "name": "Kombucha Keeper",
    "slug": "kombuchakeeper",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kombuchakeeper",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "nl.wdudokvanheel.kombuchakeeper",
      "buildNumber": "1",
      "config": {
        "usesNonExemptEncryption": false
      },
      "icon": {
        "light": "./assets/images/icon.png",
        "dark": "./assets/images/dark-icon.png",
        "tinted": "./assets/images/tinted-icon.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#f7f4f2"
      },
      "versionCode": 3,
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.bitechular.kombuchakeeper"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#f7f4f2"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
